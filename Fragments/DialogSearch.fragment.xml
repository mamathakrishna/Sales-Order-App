<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Purchase Order Found"
		title="Select Purchase Order"
		liveChange="handleSearch"
		confirm="handleClose"
		close="handleClose"
		items="{/Services}">
		<StandardListItem
			title="{name}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>